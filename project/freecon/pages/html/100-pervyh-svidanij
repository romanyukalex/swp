&#1052;&#1072;&#1096;&#1072; &#1093;&#1086;&#1076;&#1080;&#1083;&#1072; &#1085;&#1072; &#1089;&#1074;&#1080;&#1076;&#1072;&#1085;&#1080;&#1103; &#1074;&#1089;&#1077;&#1075;&#1086; 3 &#1088;&#1072;&#1079;&#1072;, &#1090;&#1097;&#1072;&#1090;&#1077;&#1083;&#1100;&#1085;&#1086; &#1074;&#1099;&#1073;&#1080;&#1088;&#1072;&#1103;, &#1089; &#1082;&#1077;&#1084;. &#1052;&#1091;&#1078;&#1095;&#1080;&#1085;&#1099; &#1077;&#1081; &#1085;&#1077; &#1086;&#1095;&#1077;&#1085;&#1100; &#1087;&#1086;&#1085;&#1088;&#1072;&#1074;&#1080;&#1083;&#1080;&#1089;&#1100;, &#1085;&#1086; &#1086;&#1085;&#1072; &#1084;&#1091;&#1095;&#1080;&#1090;&#1077;&#1083;&#1100;&#1085;&#1086; &#1087;&#1077;&#1088;&#1077;&#1078;&#1080;&#1074;&#1072;&#1083;&#1072;, &#1082;&#1086;&#1075;&#1076;&#1072; &#1086;&#1085;&#1080; &#1077;&#1081; &#1085;&#1077; &#1079;&#1074;&#1086;&#1085;&#1080;&#1083;&#1080;, &#1085;&#1077; &#1079;&#1074;&#1072;&#1083;&#1080; &#1085;&#1072; &#1087;&#1086;&#1074;&#1090;&#1086;&#1088;&#1085;&#1086;&#1077; &#1089;&#1074;&#1080;&#1076;&#1072;&#1085;&#1080;&#1077;.
<p>&#1053;&#1072;&#1082;&#1086;&#1085;&#1077;&#1094;-&#1090;&#1086; &#1080;&#1084; &#1086;&#1073;&#1077;&#1080;&#1084; &#1074;&#1089;&#1090;&#1088;&#1077;&#1090;&#1080;&#1083;&#1089;&#1103; &#1084;&#1091;&#1078;&#1095;&#1080;&#1085;&#1072; &#1084;&#1077;&#1095;&#1090;&#1099;. &#1058;&#1072;&#1085;&#1103; &#1091;&#1074;&#1080;&#1076;&#1077;&#1083;&#1072; &#1077;&#1075;&#1086;, &#1087;&#1086;&#1085;&#1103;&#1083;&#1072; &ndash; &#1101;&#1090;&#1086; &#1054;&#1053;, &#1089;&#1077;&#1088;&#1076;&#1094;&#1077; &#1079;&#1072;&#1089;&#1090;&#1091;&#1095;&#1072;&#1083;&#1086;, &#1085;&#1086;&#1075;&#1080; &#1085;&#1072;&#1095;&#1072;&#1083;&#1080; &#1076;&#1088;&#1086;&#1078;&#1072;&#1090;&#1100;, &#1086;&#1095;&#1077;&#1085;&#1100; &#1079;&#1072;&#1093;&#1086;&#1090;&#1077;&#1083;&#1086;&#1089;&#1100; &#1073;&#1086;&#1083;&#1090;&#1072;&#1090;&#1100; &#1073;&#1077;&#1079; &#1091;&#1084;&#1086;&#1083;&#1082;&#1091;, &#1088;&#1072;&#1089;&#1089;&#1082;&#1072;&#1079;&#1072;&#1090;&#1100; &#1089;&#1072;&#1084;&#1099;&#1077; &#1089;&#1084;&#1077;&#1096;&#1085;&#1099;&#1077; &#1080;&#1089;&#1090;&#1086;&#1088;&#1080;&#1080;, &#1072; &#1083;&#1091;&#1095;&#1096;&#1077;, &#1087;&#1086;&#1094;&#1077;&#1083;&#1086;&#1074;&#1072;&#1090;&#1100; &#1101;&#1090;&#1086; &#1095;&#1091;&#1076;&#1086; &#1080; &#1091;&#1090;&#1086;&#1085;&#1091;&#1090;&#1100; &#1074; &#1077;&#1075;&#1086; &#1086;&#1073;&#1098;&#1103;&#1090;&#1080;&#1103;&#1093;. &#1053;&#1077;&#1073;&#1086;&#1083;&#1100;&#1096;&#1086;&#1077; &#1079;&#1072;&#1084;&#1077;&#1096;&#1072;&#1090;&#1077;&#1083;&#1100;&#1089;&#1090;&#1074;&#1086; &#1073;&#1099;&#1089;&#1090;&#1088;&#1086; &#1087;&#1088;&#1086;&#1096;&#1083;&#1086;, &#1058;&#1072;&#1085;&#1102;&#1096;&#1072; &#1074;&#1079;&#1103;&#1083;&#1072; &#1089;&#1077;&#1073;&#1103; &#1074; &#1088;&#1091;&#1082;&#1080; &#1080; &#1087;&#1088;&#1080;&#1074;&#1099;&#1095;&#1085;&#1086; &#1089;&#1090;&#1072;&#1083;&#1072; &#1089;&#1087;&#1088;&#1072;&#1096;&#1080;&#1074;&#1072;&#109